S -> PROG
PROG -> START_PROLOG CODE END_PROLOG endOfFile

START_PROLOG -> startProlog function("declare") ( function("strict_types") = integer(1) ) ;

END_PROLOG -> eps
END_PROLOG -> endProlog 

CODE -> eps 
CODE -> INNER_SCOPE CODE
CODE -> FUNC_DECLARE CODE

BODY -> INNER_SCOPE BODY
BODY -> eps 

INNER_SCOPE -> IF_ELSE 
INNER_SCOPE -> return RETURN_VALUE ; 
INNER_SCOPE -> while ( EXP ) { BODY } 
INNER_SCOPE -> { BODY } 
INNER_SCOPE -> FUNC_CALL
INNER_SCOPE -> variable = null;
INNER_SCOPE -> variable = STRING_NT;
INNER_SCOPE -> variable = EXP;
INNER_SCOPE -> variable = FUNC_CALL;

RETURN_VALUE -> eps
RETURN_VALUE -> EXP 
RETURN_VALUE -> LITERAL
RETURN_VALUE -> STRING_NT
RETURN_VALUE -> variable

IF_ELSE -> if ( EXP ) { BODY } else { BODY } 

FUNC_CALL -> function_id ( FUNC_CALL_ARGS ) ; 
FUNC_CALL_ARGS -> eps
FUNC_CALL_ARGS -> ARG NEXT_ARG
NEXT_ARG -> eps
NEXT_ARG -> , ARG NEXT_ARG
ARG -> variable
ARG -> LITERAL 
LITERAL -> float_lit
LITERAL -> int_lit
LITERAL -> null

STRING_NT -> FIRST_STRING CONCAT_NT 
FIRST_STRING -> variable
FIRST_STRING -> string_lit

CONCAT_NT -> . FIRST_STRING CONCAT_NT
CONCAT_NT -> eps

FUNC_DECLARE -> function function_id ( FUNC_DECLARE_BODY ) : RETURN_TYPE { BODY } 
FUNC_DECLARE_BODY -> eps
FUNC_DECLARE_BODY -> ARG_TYPE variable FUNC_DECLARE_BODY
FUNC_DECLARE_BODY -> , ARG_TYPE variable FUNC_DECLARE_BODY
ARG_TYPE -> string
ARG_TYPE -> int
ARG_TYPE -> float
RETURN_TYPE -> ARG_TYPE
RETURN_TYPE -> void



terminals:
	if,
	else,
	while,
	function
	function_id,
	int,
	float,
	string,
	void,
	null,
	return,
	=
	equal,
	startProlog,
	endProlog,
	;
	(
	)
	{
	}
	.
	:
	,
	variable,
	string_lit
	int_lit
	float_lit
	endOfFile,
	eps
